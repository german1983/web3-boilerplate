{
    "name": "@german-scaffold-eth/app",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "node": "16.x"
    },
    "scripts": {
        "start": "cross-env ENVIRONMENT=DEVELOPMENT vite",
        "build": "tsc && yarn build:anttheme && cross-env ENVIRONMENT=PRODUCTION vite build",
        "build:anttheme": "yarn lessc -x -js ./src/styles/themes/light-theme.less ./public/light-theme.css && yarn lessc -x -js ./src/styles/themes/dark-theme.less ./public/dark-theme.css",
        "serve": "vite preview",
        "lint": "eslint --config ./.eslintrc --ignore-path ./.eslintignore ./src/**/*",
        "surge": "surge ./dist",
        "s3": "node ./scripts/s3.js",
        "ship": "yarn surge",
        "dev:link:components": "yalc link eth-components",
        "dev:link:hooks": "yalc link eth-hooks",
        "dev:unlink": "yalc remove eth-hooks && yalc remove eth-components"
    },
    "dependencies": {
        "@ant-design/icons": "^4.2.2",
        "@babel/runtime": "^7.15.4",
        "@emotion/react": "^11.5.0",
        "@ethersproject/address": "^5.6.0",
        "@ethersproject/bignumber": "^5.6.0",
        "@ethersproject/bytes": "^5.6.0",
        "@ethersproject/contracts": "^5.6.0",
        "@ethersproject/providers": "^5.6.1",
        "@ethersproject/solidity": "^5.6.0",
        "@ethersproject/units": "^5.6.0",
        "antd": "^4.19.3",
        "buffer": "^6.0.3",
        "eth-components": "3.0.39",
        "eth-hooks": "4.0.45",
        "ethereum-cryptography": "^1.0.3",
        "ethers": "^5.6.1",
        "process": "^0.11.10",
        "react": "^17.0.2",
        "react-blockies": "^1.4.1",
        "react-css-theme-switcher": "^0.3.0",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-qr-reader": "^3.0.0-beta-1",
        "react-router-dom": "^6.2.2",
        "react-stack-grid": "^0.7.1",
        "web3modal": "^1.9.5"
    },
    "devDependencies": {
        "@emotion/types": "^0.7.0",
        "@nomiclabs/hardhat-ethers": "^2.0.5",
        "@nomiclabs/hardhat-waffle": "^2.0.3",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^13.5.0",
        "@typechain/hardhat": "^5.0.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.1",
        "@types/pretty-time": "^1.1.2",
        "@types/qrcode.react": "^1.0.2",
        "@types/react": "^17.0.42",
        "@types/react-blockies": "^1.4.1",
        "@types/react-dom": "^17.0.14",
        "@types/react-qr-reader": "^2.1.4",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-stack-grid": "^0.7.3",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@vitejs/plugin-react": "^1.2.0",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "@walletconnect/ethereum-provider": "^1.6.5",
        "@web3-react/types": "^6.0.7",
        "autoprefixer": "^10.4.4",
        "cross-env": "^7.0.3",
        "eslint": "^8.11.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^16.1.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-formatjs": "^3.0.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "less": "^4.1.2",
        "postcss": "^8.4.0",
        "prettier": "^2.6.0",
        "rollup-plugin-polyfill-node": "^0.8.0",
        "shx": "^0.3.4",
        "stylelint": "^14.6.0",
        "stylelint-config-recommended": "^7.0.0",
        "surge": "^0.23.1",
        "ts-node": "^10.3.0",
        "typescript": "^4.6.2",
        "vite": "^2.8.6",
        "vite-plugin-babel-macros": "^1.0.6",
        "vite-plugin-externals": "^0.4.0",
        "vite-tsconfig-paths": "^3.4.1"
    },
    "browserslist": {
        "production": [
            "last 4 chrome major versions",
            "last 4 firefox major versions",
            "last 2 safari major versions"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
